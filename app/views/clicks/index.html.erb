<h1>链接点击分析</h1>

<table class="table table-striped" id="campaigns">
  <thead>
    <tr>
      <th>Member</th>
      <th>Campaign</th>
      <th>Link</th>
    </tr>
  </thead>
  <tbody>
    <% @clicks.each do |click| %>
      <tr>
        <td><%= click.member_id %></td>
        <td><%= click.campaign_id %></td>
        <td><%= click.link_id %></td>
        <td><%= click.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

